<h1>Listing lanes</h1>

<table>
  <tr>
    <th>Center</th>
    <th>Avail</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @lanes.each do |lane| %>
  <tr>
    <td><%= lane.center_id %></td>
    <td><%= lane.avail %></td>
    <td><%= link_to 'Show', lane %></td>
    <td><%= link_to 'Edit', edit_lane_path(lane) %></td>
    <td><%= link_to 'Destroy', lane, :confirm => 'Are you sure?', :method => :delete %></td>
  </tr>
<% end %>
</table>

<br />

<%= link_to 'New Lane', new_lane_path %>
